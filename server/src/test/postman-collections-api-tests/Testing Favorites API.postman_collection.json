	"item": [
		{
			"name": "Authenticate/Login as User (chris2)",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test('Authenticate as user', function(){\r",
							"    pm.response.to.have.status(200);\r",
							"})\r",
							"pm.test('Has a jwt', function(){\r",
							"    const data = pm.response.json();\r",
							"    pm.collectionVariables.set('jwt', data.jwt);\r",
							"    pm.collectionVariables.set('id', data.id);\r",
							"    return 'jwt' in data;\r",
							"})"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\r\n\t\"username\": \"{{username}}\",\r\n\t\"password\": \"{{password}}\"\r\n}"
				},
				"url": {
					"raw": "http://cs431-02.cs.rutgers.edu:8080/login/authenticate",
					"protocol": "http",
					"host": [
						"cs431-02",
						"cs",
						"rutgers",
						"edu"
					],
					"port": "8080",
					"path": [
						"login",
						"authenticate"
					]
				}
			},
			"response": []
		},
